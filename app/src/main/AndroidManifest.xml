<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crazyalarm">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="com.example.crazyalarm.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.crazyalarm.QuizActivity"></activity>
        <activity android:name="com.example.crazyalarm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.crazyalarm.ViewAlarms"></activity>

        <receiver android:name="com.example.crazyalarm.MyBroadcastReceiver" />
    </application>

</manifest>